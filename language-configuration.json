  "autoClosingPairs": [
    { "open": "(", "close": ")", "notIn": ["string"] },
    { "open": "[", "close": "]", "notIn": ["string"] },
    { "open": "{", "close": "}", "notIn": ["string"] }
  ],
  "folding": {
    "markers": {
      "start": "^(?<!(?<!(?<!(?<!(?<!(?<!(?<!(?<!(?<!(?<!(?<!(?<!\\)\\)\\)\\)\\)\\)\\)\\)\\)\\)\\)\\)[({\\[]",
      "end": "^(?<!(?<!(?<!(?<!(?<!(?<!(?<!(?<!(?<!(?<!(?<!(?<!\\)\\)\\)\\)\\)\\)\\)\\)\\)\\)\\)\\)[\\]})]"
    }
  },
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"]
  ]
}
