{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "scopeName": "source.sds",
  "name": "sds",
  "foldingStartMarker": "[\\[{]",
  "foldingStopMarker": "[\\]}]",
  "patterns": [{ "include": "#types" }],
  "repository": {
    "nocommainnum": {
      "patterns": [
        {
          "match": ",",
          "name": "type.none.sds"
        }
      ]
    },
    "types": {
      "patterns": [
        {
          "begin": "\\b(VEC[234]I)(\\()",
          "end": "(\\))",
          "captures": {
            "1": { "name": "type.name.sds" },
            "2": { "name": "containers.sds" }
          },
          "endCaptures": {
            "1": { "name": "containers.sds" }
          },
          "name": "type.int.sds",
          "patterns": [
            {
              "match": ",",
              "name": "separator.comma.sds"
            }
          ]
        },
        {
          "begin": "\\b(COLOR)(\\()",
          "end": "(\\))",
          "captures": {
            "1": { "name": "type.name.sds" },
            "2": { "name": "containers.sds" }
          },
          "endCaptures": {
            "1": { "name": "containers.sds" }
          },
          "name": "type.float.sds",
          "patterns": [
            {
              "match": ",",
              "name": "separator.comma.sds"
            }
          ]
        },
        {
          "begin": "\\b(VEC[234])(\\()",
          "end": "(\\))",
          "captures": {
            "1": { "name": "type.name.sds" },
            "2": { "name": "containers.sds" }
          },
          "endCaptures": {
            "1": { "name": "containers.sds" }
          },
          "name": "type.float.sds",
          "patterns": [
            {
              "match": ",",
              "name": "separator.comma.sds"
            }
          ]
        },
        {
          "begin": "\\b(RECT2)(\\()",
          "end": "(\\))",
          "captures": {
            "1": { "name": "type.name.sds" },
            "2": { "name": "containers.sds" }
          },
          "endCaptures": {
            "1": { "name": "containers.sds" }
          },
          "name": "type.float.sds",
          "patterns": [
            {
              "match": ",",
              "name": "separator.comma.sds"
            }
          ]
        },
        {
          "begin": "\\b(RECT2I)(\\()",
          "end": "(\\))",
          "captures": {
            "1": { "name": "type.name.sds" },
            "2": { "name": "containers.sds" }
          },
          "endCaptures": {
            "1": { "name": "containers.sds" }
          },
          "name": "type.int.sds",
          "patterns": [
            {
              "match": ",",
              "name": "separator.comma.sds"
            }
          ]
        },
        {
          "begin": "\\b(STR)(\\()",
          "end": "(?<!(?<!(?<!(?<!(?<!(?<!(?<!(?<!(?<!(?<!(?<!(?<!\\\\)\\\\)\\\\)\\\\)\\\\)\\\\)\\\\)\\\\)\\\\)\\\\)\\\\)\\\\)(\\))",
          "captures": {
            "1": { "name": "type.name.sds" },
            "2": { "name": "containers.sds" }
          },
          "endCaptures": {
            "1": { "name": "containers.sds" }
          },
          "contentName": "string.sds",
          "patterns": [
            {
              "match": "(\\\\)(\\\\)(?!\\))|(\\\\)(\\))|(\\\\)(?!\\\\)|(\\\\)(\\\\)(\\))",
              "name": "",
              "captures": {
                "6": { "name": "null.sds" },
                "7": { "name": "escaped.sds" },
                "8": { "name": "error.sds" },
                "1": { "name": "null.sds" },
                "2": { "name": "escaped.sds" },
                "3": { "name": "null.sds" },
                "4": { "name": "escaped.sds" },
                "5": { "name": "escaped.sds" }
              }
            },
            {
              "match": ".",
              "name": "type.str.sds"
            }
          ]
        },
        {
          "match": "\\b(NULL)(\\(\\))",
          "name": "type.name.sds"
        },
        {
          "begin": "\\b(STRNAME)(\\()",
          "end": "(?<!(?<!(?<!(?<!(?<!(?<!(?<!(?<!(?<!(?<!(?<!(?<!\\\\)\\\\)\\\\)\\\\)\\\\)\\\\)\\\\)\\\\)\\\\)\\\\)\\\\)\\\\)(\\))",
          "captures": {
            "1": { "name": "type.name.sds" },
            "2": { "name": "containers.sds" }
          },
          "endCaptures": {
            "1": { "name": "containers.sds" }
          },
          "patterns": [
            {
              "match": "\\\\.",
              "name": "escaped.sds"
            },
            {
              "match": ".",
              "name": "type.strname.sds"
            }
          ]
        },
        {
          "begin": "\\b(INT)(\\()",
          "end": "(\\))",
          "captures": {
            "1": { "name": "type.name.sds" },
            "2": { "name": "containers.sds" }
          },
          "endCaptures": {
            "1": { "name": "containers.sds" }
          },
          "name": "type.int.sds"
        },
        {
          "begin": "\\b(FLOAT)(\\()",
          "end": "(\\))",
          "captures": {
            "1": { "name": "type.name.sds" },
            "2": { "name": "containers.sds" }
          },
          "endCaptures": {
            "1": { "name": "containers.sds" }
          },
          "name": "type.float.sds"
        },
        {
          "begin": "\\b(BOOL)(\\()",
          "end": "(\\))",
          "captures": {
            "1": { "name": "type.name.sds" },
            "2": { "name": "containers.sds" }
          },
          "endCaptures": {
            "1": { "name": "containers.sds" }
          },
          "name": "type.bool.sds"
        },
        {
          "match": "[\\]}{\\[]",
          "name": "containers.sds"
        }
      ]
    }
  }
}
