{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "scopeName": "source.sds",
  "name": "sds",
  "foldingStartMarker": "[\\[{]",
  "foldingStopMarker": "[\\]}]",
  "patterns": [{ "include": "#types" }],
  "repository": {
    "nocommainnum": {
      "patterns": [
        {
          "match": ",",
          "name": "sds.type.none"
        }
      ]
    },
    "types": {
      "patterns": [
        {
          "begin": "\\b(VEC[234]I)(\\()",
          "end": "(\\))",
          "captures": {
            "1": { "name": "sds.type.name" },
            "2": { "name": "sds.containers" }
          },
          "endCaptures": {
            "1": { "name": "sds.containers" }
          },
          "name": "sds.type.int",
          "patterns": [
            {
              "match": ",",
              "name": "sds.separator.comma"
            }
          ]
        },
        {
          "begin": "\\b(COLOR)(\\()",
          "end": "(\\))",
          "captures": {
            "1": { "name": "sds.type.name" },
            "2": { "name": "sds.containers" }
          },
          "endCaptures": {
            "1": { "name": "sds.containers" }
          },
          "name": "sds.type.float",
          "patterns": [
            {
              "match": ",",
              "name": "sds.separator.comma"
            }
          ]
        },
        {
          "begin": "\\b(VEC[234])(\\()",
          "end": "(\\))",
          "captures": {
            "1": { "name": "sds.type.name" },
            "2": { "name": "sds.containers" }
          },
          "endCaptures": {
            "1": { "name": "sds.containers" }
          },
          "name": "sds.type.float",
          "patterns": [
            {
              "match": ",",
              "name": "sds.separator.comma"
            }
          ]
        },
        {
          "begin": "\\b(RECT2)(\\()",
          "end": "(\\))",
          "captures": {
            "1": { "name": "sds.type.name" },
            "2": { "name": "sds.containers" }
          },
          "endCaptures": {
            "1": { "name": "sds.containers" }
          },
          "name": "sds.type.float",
          "patterns": [
            {
              "match": ",",
              "name": "sds.separator.comma"
            }
          ]
        },
        {
          "begin": "\\b(RECT2I)(\\()",
          "end": "(\\))",
          "captures": {
            "1": { "name": "sds.type.name" },
            "2": { "name": "sds.containers" }
          },
          "endCaptures": {
            "1": { "name": "sds.containers" }
          },
          "name": "sds.type.int",
          "patterns": [
            {
              "match": ",",
              "name": "sds.separator.comma"
            }
          ]
        },
        {
          "begin": "\\b(STR)(\\()",
          "end": "(?<!(?<!(?<!(?<!(?<!(?<!(?<!(?<!(?<!(?<!(?<!(?<!\\\\)\\\\)\\\\)\\\\)\\\\)\\\\)\\\\)\\\\)\\\\)\\\\)\\\\)\\\\)(\\))",
          "captures": {
            "1": { "name": "sds.type.name" },
            "2": { "name": "sds.containers" }
          },
          "endCaptures": {
            "1": { "name": "sds.containers" }
          },
          "patterns": [
            {
              "match": "\\\\.",
              "name": "sds.escaped"
            },
            {
              "match": ".",
              "name": "sds.type.str"
            }
          ]
        },
        {
          "begin": "\\b(STRNAME)(\\()",
          "end": "(?<!(?<!(?<!(?<!(?<!(?<!(?<!(?<!(?<!(?<!(?<!(?<!\\\\)\\\\)\\\\)\\\\)\\\\)\\\\)\\\\)\\\\)\\\\)\\\\)\\\\)\\\\)(\\))",
          "captures": {
            "1": { "name": "sds.type.name" },
            "2": { "name": "sds.containers" }
          },
          "endCaptures": {
            "1": { "name": "sds.containers" }
          },
          "patterns": [
            {
              "match": "\\\\.",
              "name": "sds.escaped"
            },
            {
              "match": ".",
              "name": "sds.type.strname"
            }
          ]
        },
        {
          "begin": "\\b(INT)(\\()",
          "end": "(\\))",
          "captures": {
            "1": { "name": "sds.type.name" },
            "2": { "name": "sds.containers" }
          },
          "endCaptures": {
            "1": { "name": "sds.containers" }
          },
          "name": "sds.type.int"
        },
        {
          "begin": "\\b(FLOAT)(\\()",
          "end": "(\\))",
          "captures": {
            "1": { "name": "sds.type.name" },
            "2": { "name": "sds.containers" }
          },
          "endCaptures": {
            "1": { "name": "sds.containers" }
          },
          "name": "sds.type.float"
        },
        {
          "begin": "\\b(BOOL)(\\()",
          "end": "(\\))",
          "captures": {
            "1": { "name": "sds.type.name" },
            "2": { "name": "sds.containers" }
          },
          "endCaptures": {
            "1": { "name": "sds.containers" }
          },
          "name": "sds.type.bool"
        },
        {
          "match": "[\\]}{\\[]",
          "name": "sds.containers"
        }
      ]
    }
  }
}
